{
  "icon_theme": {
    "mode": "system",
    "light": "Warm Charmed Icons",
    "dark": "Warm Charmed Icons",
  },
  "base_keymap": "Cursor",
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Aura Dark",
  },
  "cursor_blink": false,
  "buffer_font_family": "CaskaydiaMono Nerd Font",
  "buffer_font_weight": 300,
  "ui_font_family": "CaskaydiaMono Nerd Font",
  "ui_font_weight": 300,
  "ui_font_size": 15.0,
  "buffer_font_size": 15.0,
  "features": {
    "edit_prediction_provider": "zed",
  },
  "show_whitespaces": "all",
  "terminal": {
    "shell": "system",
    "copy_on_select": true,
    "font_family": "CaskaydiaMono Nerd Font Mono",
    "font_features": {
      "liga": true,
    },
    "font_size": 15.0,
    "option_as_meta": true,
    "working_directory": "current_project_directory",
  },
  "vim": {
    "use_system_clipboard": "always",
    "use_smartcase_find": true,
  },
  "relative_line_numbers": "enabled",
  "vertical_scroll_margin": 100,
  "project_panel": {
    "default_width": 400,
    "hide_root": true,
    "auto_fold_dirs": false,
    "starts_open": false,
    "git_status": false,
    "sticky_scroll": false,
    "scrollbar": {
      "show": "never",
    },
  },
  "outline_panel": {
    "default_width": 300,
  },
  "file_finder": {
    "modal_max_width": "large",
  },
  "gutter": {
    "min_line_number_digits": 0,
    "folds": false,
    "runnables": false,
  },
  "agent_buffer_font_size": 15,
  "buffer_line_height": "comfortable",
}
